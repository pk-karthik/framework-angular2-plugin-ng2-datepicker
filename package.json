    "roll": "node ./scripts/index.js --build",
    "roll:prerender": "npm run roll && npm run prerender",
    "prerender": "node ./scripts/index.js --prerender",
    "clean": "./scripts/clean.sh",
    "test": "karma start config/karma.conf.js",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "tslint": "tslint",
    "sass": "node-sass src/ng2-datepicker/ng2-datepicker.sass src/ng2-datepicker/ng2-datepicker.css",
    "build": "npm run sass && ./node_modules/.bin/ngc -p tsconfig.build.json",
    "prepublish": "npm run build"
  },
